{
  "name": "duration-to-ms",
  "version": "0.1.6",
  "description": "Library for converting duration string to ms",
  "directories": {
    "test": "tests"
  },
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/esnext/index.js",
  "types": "dist/src/index.d.ts",
  "browser": {
    "./src/index.ts": "./src/index-browser.ts",
    "./dist/esm/index.js": "./dist/esm/index-browser.js",
    "./dist/esnext/index.js": "./dist/esnext/index-browser.js",
    "./dist/src/index.js": "./dist/src/index-browser.js"
  },
  "keywords": [
    "time",
    "seconds",
    "milliseconds",
    "duration",
    "convert",
    "parse"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint **/*.js",
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc --build tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "preversion": "npm run lint && npm run build && npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmytryS/duration-to-ms.git"
  },
  "author": "Dmytro Shvaika",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DmytryS/duration-to-ms/issues"
  },
  "homepage": "https://github.com/DmytryS/duration-to-ms#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
